  "author": "James Sumners <james.sumners@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-url-data/issues"
  },
  "homepage": "https://github.com/fastify/fastify-url-data#readme",
  "devDependencies": {
    "fastify": "^0.30.3",
    "pre-commit": "^1.2.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "tap": "^10.7.2",
    "typescript": "~2.5.3"
  },
  "dependencies": {
    "fastify-plugin": "^0.1.1",
    "uri-js": "^3.0.2"
  }
