{
  "name": "fastify-url-data",
  "version": "1.1.0",
  "description": "A Fastify plugin to provide access to URI components",
  "main": "plugin.js",
  "scripts": {
    "test": "npm run test-ts && tap 'test/**/*.test.js'",
    "test-ci": "tap --cov 'test/**/*.test.js'",
    "test-ts": "tsc test/types.test.ts --target es6 --module commonjs --noEmit",
    "lint": "standard | snazzy",
